<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>Ink Touch - Large Format Printing</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta content="" name="keywords" />
    <meta content="" name="description" />

    <!-- Favicon -->
    <link href="../../img/ink.jpg" rel="icon" />

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Heebo:wght@400;500;600;700&display=swap"
      rel="stylesheet"
    />

    <!-- Icon Font Stylesheet -->
    <link
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css"
      rel="stylesheet"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css"
      rel="stylesheet"
    />

    <!-- Libraries Stylesheet -->
    <link
      href="../../lib/owlcarousel/assets/owl.carousel.min.css"
      rel="stylesheet"
    />
    <link
      href="../../lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css"
      rel="stylesheet"
    />

    <!-- Customized Bootstrap Stylesheet -->
    <link href="../../css/bootstrap.min.css" rel="stylesheet" />

    <!-- Template Stylesheet -->
    <link href="../../css/style.css" rel="stylesheet" />
    <link href="../../css/form.css" rel="stylesheet" />
    <link href="../../css/viewBtn.css" rel="stylesheet" />
  </head>

  <body>
    <div class="container-xxl position-relative bg-white d-flex p-0">
      <%- include (`../partials/adminHeader`); -%>

      <!-- Content Start -->
      <div class="content">
        <!-- Navbar Start -->
        <%- include (`../partials/adminNav`); -%>
        <!-- Navbar End -->

        <!-- Report start -->

        <div class="container-fluid pt-4 px-4">
          <div class="bg-light text-center rounded p-4">
            <div class="containereds">
              <header>Search Report</header>
              <div class="row">
                <div class="card">
                  <div class="card-body">
                    <div class="tab-content">
                      <form id="searchForm" method="POST">
                        <div class="form-group">
                          <div class="row">
                            <label class="col-sm-3 control-label"
                              >Start Date</label
                            >
                            <div class="col-sm-9">
                              <input
                                type="date"
                                class="form-control"
                                name="fromdate"
                                required
                              />
                            </div>
                          </div>
                        </div>

                        <div class="form-group">
                          <div class="row">
                            <label class="col-sm-3 control-label"
                              >End Date</label
                            >
                            <div class="col-sm-9">
                              <input
                                type="date"
                                class="form-control"
                                name="todate"
                                required
                              />
                            </div>
                          </div>
                        </div>

                        <center>
                          <div
                            class="col-xl-4 col-lg-4 col-md-12 col-sm-12 col-12 mb-2"
                          >
                            <button
                              class="btn btn-primary"
                              type="submit"
                              name="search"
                              id="searchBtn"
                            >
                              Search
                            </button>
                          </div>
                        </center>
                      </form>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- Report start -->

        <!-- Report result start -->
        <div class="container-fluid pt-4 px-4">
          <div class="bg-light text-center rounded p-4">
            <div class="container">
              <header>Report Result</header>
              <div class="row">
                <div class="col-lg-12">
                  <div class="card">
                    <div class="card-body">
                      <div class="table-responsive">
                        <table
                          id="datatable-buttons"
                          class="table table-striped table-bordered dt-responsive nowrap"
                          style="width: 100%"
                        >
                          <thead>
                            <tr>
                              <th>S/N</th>
                              <th>Action</th>
                              <th>Operator</th>
                              <th>Date</th>
                              <th>Ref</th>
                              <th>Category</th>
                              <th>Material</th>
                              <th>QTY</th>
                              <th>Narration</th>
                              <th>Status</th>
                            </tr>
                          </thead>
                          <tbody></tbody>
                        </table>
                        <div
                          id="pagination"
                          class="pagination-controls mt-3"
                        ></div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- report result end -->

        <!-- Footer Start -->
        <%- include (`../partials/adminFooter`); -%>
        <!-- Footer End -->
      </div>
      <!-- Content End -->

      <!-- Back to Top -->
      <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"
        ><i class="bi bi-arrow-up"></i
      ></a>
    </div>
    <script src="../../mail/search-report.js"></script>
  </body>
</html>
